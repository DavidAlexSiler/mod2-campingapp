<h1>Create a user profile </h1>

<%= form_for(@user) do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>
  <div>
  <%= f.label :age %>
  <%= f.text_field :age %>
  <div>
  <%= f.label :bio %>
  <%= f.text_area :bio %>
  <div>
  <%= f.label :Image %>
  <%= f.text_field :img_url %>
  <div>
  <%= f.label :username %>
  <%= f.text_field :username %>
  <div>
  <%= f.label :password %>
  <%= f.password_field :password %>
  <div>

  <%= f.submit %>
<% end %>
